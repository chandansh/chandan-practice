package com.coleg.model;

import com.liferay.portal.kernel.bean.AutoEscapeBeanHandler;
import com.liferay.portal.kernel.util.StringBundler;
import com.liferay.portal.model.impl.BaseModelImpl;

import java.io.Serializable;

import java.lang.reflect.Proxy;

/**
 * @author chandan
 */
public class EmployeeClp extends BaseModelImpl<Employee> implements Employee {
	public EmployeeClp() {
	}

	public String getPrimaryKey() {
		return _eId;
	}

	public void setPrimaryKey(String pk) {
		setEId(pk);
	}

	public Serializable getPrimaryKeyObj() {
		return _eId;
	}

	public String getEId() {
		return _eId;
	}

	public void setEId(String eId) {
		_eId = eId;
	}

	public String getName() {
		return _name;
	}

	public void setName(String name) {
		_name = name;
	}

	public String getAddress() {
		return _address;
	}

	public void setAddress(String address) {
		_address = address;
	}

	public Employee toEscapedModel() {
		if (isEscapedModel()) {
			return this;
		}
		else {
			return (Employee)Proxy.newProxyInstance(Employee.class.getClassLoader(),
				new Class[] { Employee.class }, new AutoEscapeBeanHandler(this));
		}
	}

	public Object clone() {
		EmployeeClp clone = new EmployeeClp();

		clone.setEId(getEId());
		clone.setName(getName());
		clone.setAddress(getAddress());

		return clone;
	}

	public int compareTo(Employee employee) {
		String pk = employee.getPrimaryKey();

		return getPrimaryKey().compareTo(pk);
	}

	public boolean equals(Object obj) {
		if (obj == null) {
			return false;
		}

		EmployeeClp employee = null;

		try {
			employee = (EmployeeClp)obj;
		}
		catch (ClassCastException cce) {
			return false;
		}

		String pk = employee.getPrimaryKey();

		if (getPrimaryKey().equals(pk)) {
			return true;
		}
		else {
			return false;
		}
	}

	public int hashCode() {
		return getPrimaryKey().hashCode();
	}

	public String toString() {
		StringBundler sb = new StringBundler(7);

		sb.append("{eId=");
		sb.append(getEId());
		sb.append(", name=");
		sb.append(getName());
		sb.append(", address=");
		sb.append(getAddress());
		sb.append("}");

		return sb.toString();
	}

	public String toXmlString() {
		StringBundler sb = new StringBundler(13);

		sb.append("<model><model-name>");
		sb.append("com.coleg.model.Employee");
		sb.append("</model-name>");

		sb.append(
			"<column><column-name>eId</column-name><column-value><![CDATA[");
		sb.append(getEId());
		sb.append("]]></column-value></column>");
		sb.append(
			"<column><column-name>name</column-name><column-value><![CDATA[");
		sb.append(getName());
		sb.append("]]></column-value></column>");
		sb.append(
			"<column><column-name>address</column-name><column-value><![CDATA[");
		sb.append(getAddress());
		sb.append("]]></column-value></column>");

		sb.append("</model>");

		return sb.toString();
	}

	private String _eId;
	private String _name;
	private String _address;
}