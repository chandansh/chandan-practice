<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">
<service-builder package-path="com.coleg">
	<author>supriya</author>
	<namespace>myportlet_coleg</namespace>

	<entity name="Employee" local-service="true" remote-service="true">

		<column name="id" type="String" primary="true"></column>
		<column name="name" type="String" />
		<column name="address" type="String" />

	</entity>
	<entity name="Customer" local-service="true" remote-service="true">
		<column name="id" type="String" primary="true"></column>
		<column name="name" type="String"></column>
		<column name="company" type="String"></column>
		<column name="address" type="String"></column>
	</entity>
</service-builder>
